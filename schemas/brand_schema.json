{
    "$defs": {
        "CountryCode": {
            "anyOf": [
                {
                    "maxLength": 2,
                    "minLength": 2,
                    "pattern": "^([A-Z]{2})$",
                    "type": "string"
                },
                {
                    "const": "Unknown",
                    "type": "string"
                }
            ],
            "description": "String type for ISO 3166-1 alpha-2 country codes.",
            "title": "CountryCode"
        },
        "LimitedString": {
            "description": "String type with a maximum length constraint.",
            "maxLength": 1000,
            "title": "LimitedString",
            "type": "string"
        }
    },
    "additionalProperties": false,
    "description": "Brand/manufacturer information.",
    "properties": {
        "brand": {
            "$ref": "#/$defs/LimitedString",
            "description": "The name of the filament manufacture"
        },
        "website": {
            "$ref": "#/$defs/LimitedString",
            "description": "The website of the filament manufacture"
        },
        "logo": {
            "$ref": "#/$defs/LimitedString",
            "description": "A link to the logo for this brand"
        },
        "origin": {
            "$ref": "#/$defs/CountryCode",
            "description": "The country of origin"
        }
    },
    "required": [
        "brand",
        "website",
        "logo",
        "origin"
    ],
    "title": "Brand",
    "type": "object"
}
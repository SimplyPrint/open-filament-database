{
    "$defs": {
        "CountryCode": {
            "anyOf": [
                {
                    "maxLength": 2,
                    "minLength": 2,
                    "pattern": "^([A-Z]{2})$",
                    "type": "string"
                },
                {
                    "const": "Unknown",
                    "type": "string"
                }
            ],
            "description": "String type for ISO 3166-1 alpha-2 country codes.",
            "title": "CountryCode"
        },
        "FilamentSize": {
            "additionalProperties": false,
            "description": "Size/weight variant of a filament color.",
            "properties": {
                "filament_weight": {
                    "default": 1000,
                    "description": "The weight of the filament alone (in grams)",
                    "title": "Filament Weight",
                    "type": "number"
                },
                "diameter": {
                    "default": 1.75,
                    "description": "The diameter of the filament (in mm)",
                    "title": "Diameter",
                    "type": "number"
                },
                "empty_spool_weight": {
                    "anyOf": [
                        {
                            "type": "number"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "The weight of a spool with no filament (in grams)",
                    "title": "Empty Spool Weight"
                },
                "spool_core_diameter": {
                    "anyOf": [
                        {
                            "type": "number"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "The diameter of the core of the spool",
                    "title": "Spool Core Diameter"
                },
                "ean": {
                    "anyOf": [
                        {
                            "$ref": "#/$defs/LimitedString"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null
                },
                "article_number": {
                    "anyOf": [
                        {
                            "$ref": "#/$defs/LimitedString"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null
                },
                "barcode_identifier": {
                    "anyOf": [
                        {
                            "$ref": "#/$defs/LimitedString"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null
                },
                "nfc_identifier": {
                    "anyOf": [
                        {
                            "$ref": "#/$defs/LimitedString"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null
                },
                "qr_identifier": {
                    "anyOf": [
                        {
                            "$ref": "#/$defs/LimitedString"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null
                },
                "discontinued": {
                    "anyOf": [
                        {
                            "type": "boolean"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "title": "Discontinued"
                },
                "purchase_links": {
                    "anyOf": [
                        {
                            "items": {
                                "$ref": "#/$defs/PurchaseLink"
                            },
                            "type": "array"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "A list of places to purchase this filament",
                    "title": "Purchase Links"
                }
            },
            "title": "FilamentSize",
            "type": "object"
        },
        "LimitedString": {
            "description": "String type with a maximum length constraint.",
            "maxLength": 1000,
            "title": "LimitedString",
            "type": "string"
        },
        "PurchaseLink": {
            "description": "Purchase link for a specific size.",
            "properties": {
                "store_id": {
                    "$ref": "#/$defs/LimitedString"
                },
                "url": {
                    "$ref": "#/$defs/LimitedString"
                },
                "affiliate": {
                    "title": "Affiliate",
                    "type": "boolean"
                },
                "spool_refill": {
                    "default": false,
                    "description": "Indicates if this is a refill for a reusable spool",
                    "title": "Spool Refill",
                    "type": "boolean"
                },
                "ships_from": {
                    "anyOf": [
                        {
                            "items": {
                                "$ref": "#/$defs/CountryCode"
                            },
                            "type": "array"
                        },
                        {
                            "$ref": "#/$defs/CountryCode"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "A list of locations the shop ships from. Defining this here will override the definition from the shop.",
                    "title": "Ships From"
                },
                "ships_to": {
                    "anyOf": [
                        {
                            "items": {
                                "$ref": "#/$defs/CountryCode"
                            },
                            "type": "array"
                        },
                        {
                            "$ref": "#/$defs/CountryCode"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "A list of locations the shop ships to. Defining this here will override the definition from the shop.",
                    "title": "Ships To"
                }
            },
            "required": [
                "store_id",
                "url",
                "affiliate"
            ],
            "title": "PurchaseLink",
            "type": "object"
        }
    },
    "description": "Array of filament sizes - models the sizes.json schema.",
    "items": {
        "$ref": "#/$defs/FilamentSize"
    },
    "minItems": 1,
    "title": "FilamentSizeArray",
    "type": "array"
}